{% extends 'admin/adminbase.html.twig' %}

{% block body %}
<h1>{{ title }}</h1>
    <p><a href="{{ path('app_logout') }}">Выйти</a></p>
    <p><a href="{{ path('admin_user_create') }}">Создать пользователя</a></p>
    <h2>Список пользователей</h2>
    <ul>
        {% for user in users %}
        <li>{{ user.email }}, {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>
{% endblock %}